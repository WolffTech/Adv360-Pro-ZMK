macro_screensave: macro_screensave{
compatible = "zmk,behavior-macro";
label = "macro_screensave";
#binding-cells = <0>;
bindings = <&sk LEFT_META>, <&kp SPACE>, <&sk LEFT_META>, <&kp S>, <&kp C>, <&kp R>, <&kp E>, <&kp ENTER>;
};
